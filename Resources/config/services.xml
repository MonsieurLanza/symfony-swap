<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="florianv_swap.builder" class="Swap\Builder">
            <argument type="collection" /> <!-- config -->
        </service>

        <service id="florianv_swap.swap" class="Swap\Swap">
            <factory service="florianv_swap.builder" method="build"/>
        </service>
    </services>

</container>
